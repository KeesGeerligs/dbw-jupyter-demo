{
    "ops": [
      {
        "id": "pytorch",
        "args": {
          "cmd": [
            "jupyter",
            "notebook",
            "--ip=0.0.0.0",
            "--port=8888",
            "--no-browser",
            "--allow-root",
            "--ServerApp.token=''",
            "--ServerApp.password=''"
          ],
          "gpu": true,
          "image": "jupyter/datascience-notebook:x86_64-ubuntu-22.04",
          "expose": 8888
        },
        "type": "container/run"
      }
    ],
    "meta": {
      "trigger": "dashboard",
      "system_requirements": {
      }
    },
    "type": "container",
    "version": "0.1"
  }